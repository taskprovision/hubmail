# Performance Benchmarks Example Environment Variables

# Logging Configuration
LOG_LEVEL=INFO
LOG_DIR=./logs
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# Output Configuration
OUTPUT_DIR=./output
BENCHMARK_DIR=./benchmarks
REPORT_FORMAT=json,csv

# Benchmark Configuration
ITERATIONS=10
WARMUP_ITERATIONS=2
COMPARE_WITH_FRAMEWORKS=true
GENERATE_PLOTS=true

# Frameworks to Compare (comma-separated)
FRAMEWORKS_TO_COMPARE=prefect,airflow,luigi

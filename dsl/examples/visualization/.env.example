# Visualization Example Configuration

# Output directory for generated visualizations
OUTPUT_DIR=output

# Visualization settings
VISUALIZATION_FORMAT=html
VISUALIZATION_THEME=default
INCLUDE_TASK_CODE=true
INCLUDE_TASK_DETAILS=true

# Mermaid diagram settings
MERMAID_THEME=default
MERMAID_DIRECTION=TB

# Server settings for interactive visualization
SERVER_PORT=8000
OPEN_BROWSER_ON_START=true

# Logging settings
LOG_LEVEL=INFO
LOG_FILE=logs/visualization.log

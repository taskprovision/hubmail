# Parallel Tasks Example Configuration

# Output directory for generated results
OUTPUT_DIR=output

# Parallel execution settings
MAX_WORKERS=4
CHUNK_SIZE=250
ENABLE_DYNAMIC_SCALING=true

# Performance monitoring
ENABLE_PERFORMANCE_MONITORING=true
PERFORMANCE_LOG_FILE=logs/performance.log

# Logging settings
LOG_LEVEL=INFO
LOG_FILE=logs/parallel_tasks.log
